eight: 100vh;\n  min-width: 400px;\n  min-height: 800px;\n  background: rgba(20, 29, 37, .9);\n  z-index: 1;\n  padding: 30px;\n  display: flex;\n  flex-flow: column;\n  justify-content: flex-start;\n  line-height: 6rem;\n  align-content: flex-start;\n  transition: .5s;\n}\n\n#footer {\n  grid-area: footer;\n  display: flex;\n  flex-flow: wrap;\n  padding: 10px;\n  justify-content: center;\n}\n\n.grid-item {\n  min-height: fit-content;\n  padding: 4px 10px;\n  border: 1.5px dashed grey;\n  display: flex;\n  flex-grow: 1;\n  margin: 10px auto;\n  transition-duration: .3s;\n  width: 60vw;\n  min-width: 60vw;\n\n  &:hover {\n    border: 1.5px dashed #2980b9;\n    box-shadow: 0px 2px 4px 0px rgba(20, 29, 37, 0.2);\n\n    h1 {\n      color: #2980b9;\n    }\n  }\n\n  .photo {\n    h1:{\n      padding: 4px;\n      flex: 3;\n      left:auto;\n    }\n\n    img {\n    max-width: 150px;\n    min-width: 150px;\n    }\n  }\n\n  &.user {\n    flex-flow: column wrap;\n    /* width: 90vw; */\n    display: flex;\n  }\n}\n\n.tab {\n  padding-left: 20px;\n  flex-basis: 20%;\n  display: flex;\n  text-decoration: none;\n  justify-content: flex-start;\n  align-content: flex-end;\n  align-items: center;\n  font-size: 3rem;\n  color: white;\n  letter-spacing: 1px;\n  transition: .2s;\n\n  &:hover {\n    color: #2980b9;\n    letter-spacing: 2px;\n  }\n}\n\n.hamburger {\n  width: 50px;\n  height: 50px;\n  cursor: pointer;\n  z-index: 2;\n  margin-left: 10px;\n\n  &:hover > * {\n    fill: #2980b9;\n  }\n}\n\n.btn {\n  -webkit-appearance: none;\n  -moz-appearance: none;\n  cursor: pointer;\n  border-radius: 5px;\n}\n\n.changeLimits {\n  position: absolute;\n  right: 20px;\n  cursor: pointer;\n  border-radius: 5px;\n  display: flex;\n  flex-flow: column;\n  button:focus {\n    outline: 0;\n  }\n  button:active {\n    color: #2980b9;\n  }\n}\n\n.changeLimitBtn {\n  -webkit-appearance: none;\n  -moz-appearance: none;\n  cursor: pointer;\n  border-radius: 5px;\n  width: 200px;\n  height: 40px;\n  transition-duration: .2s;\n  letter-spacing: 1px;\n\n  &:hover {\n    box-shadow: 0px 1px 2px 0px rgba(20, 29, 37, 0.2);\n    letter-spacing: 2px;\n    color: #2980b9;\n  }\n}\n\n.limits {\n  z-index: 1;\n  position: relative;\n  display: flex;\n  flex-flow: column;\n  justify-content: space-around;\n  align-content: center;\n  grid-area: 1 / 3;\n}\n\n.limit {\n  z-index: 1;\n  padding: 4px;\n  box-shadow: 0px 8px 16px 0px rgba(20, 29, 37, 0.2);\n  width: 100%;\n  flex-basis: 20%;\n  font-size: 24px;\n  align-items: center;\n  grid-area: 1 / 3;\n  color: rgba(20, 29, 37, 1);\n  background-color: white;\n  letter-spacing: 1px;\n  transition: .5s;\n\n  &:hover {\n    box-shadow: 0px 1px 2px 0px rgba(20, 29, 37, 0.2);\n    letter-spacing: inherit;\n    color: #2980b9;\n  }\n}\n\n/* Flexbox Grid */\n.row {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n}\n\n.column {\n  flex-basis: 100%;\n}\n\n.left {\n  margin-right: auto;\n}\n\n.right {\n  margin-left: auto;\n}\n\n/* Media Qs */\n\n@media (min-width: 668px) {\n  * {\n    font-size: 18px;\n  }\n\n  #layout {\n    height: 100vh;\n    display: grid;\n    grid-template-columns: .1fr 3fr 1fr;\n    grid-gap: 10px;\n    grid-template-areas: \". header header\" \"sidebar content content\" \"footer footer footer\";\n  }\n\n  .hamburger {\n    display: none;\n  }\n\n  .tab {\n    color: inherit;\n    font-size: 1.5rem;\n    line-height: 2.5rem;\n  }\n\n  #sidebar {\n    grid-area: sidebar;\n    min-width: 225px;\n    min-height: inherit;\n    height: 80vh;\n    background: inherit;\n    padding: 30px;\n    display: flex;\n    flex-flow: column;\n    justify-content: flex-start;\n    align-content: flex-start;\n    transition: .2s;\n  }\n\n  .grid-item {\n    max-width: 75vw;\n\n    &.user {\n      max-width: 50vw;\n    }\n  }\n}\n"},"lineCount":301}},"error":null,"hash":"419e5af65897bd0868452a15c58b3cad","cacheData":{"env":{}}}